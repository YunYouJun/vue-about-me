{
  "name": "vue-about-me",
  "version": "1.0.3",
  "description": "A Vue component for displaying About Me Info.",
  "main": "./dist/vue-about-me.umd.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "src",
    "index.d.ts"
  ],
  "module": "./dist/vue-about-me.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-about-me.es.js",
      "require": "./dist/vue-about-me.umd.js"
    },
    "./style": {
      "import": "./dist/style.css"
    },
    "./sfc": {
      "import": "./src/VueAboutMe.vue"
    }
  },
  "browser": {
    "./sfc": "./src/VueAboutMe.vue"
  },
  "style": "./dist/style.css",
  "repository": "https://github.com/YunYouJun/vue-about-me.git",
  "author": {
    "email": "me@yunyoujun.cn",
    "name": "YunYouJun",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:example": "vite build --mode example",
    "serve": "vite preview",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "vue",
    "about",
    "me",
    "link",
    "copyright"
  ],
  "devDependencies": {
    "@iconify/json": "^1.1.358",
    "@types/node": "^15.12.4",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "typescript": "^4.3.4",
    "vite": "2.3.7",
    "vite-plugin-components": "^0.11.2",
    "vite-plugin-icons": "^0.6.3"
  },
  "dependencies": {
    "sass": "^1.35.1",
    "vue": "^3.1.1"
  }
