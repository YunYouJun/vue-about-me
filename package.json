{
  "name": "vue-about-me",
  "type": "module",
  "version": "1.3.4",
  "packageManager": "pnpm@9.15.2",
  "description": "A Vue component for displaying About Me Info.",
  "author": {
    "email": "me@yunyoujun.cn",
    "name": "YunYouJun",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MIT",
  "homepage": "https://yunyoujun.github.io/vue-about-me/",
  "repository": "https://github.com/YunYouJun/vue-about-me.git",
  "keywords": [
    "vue",
    "about",
    "me",
    "link",
    "copyright"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-about-me.mjs",
      "require": "./dist/vue-about-me.cjs"
    },
    "./style.css": "./dist/vue-about-me.css",
    "./sfc": "./src/VueAboutMe.vue"
  },
  "main": "./dist/vue-about-me.cjs",
  "module": "./dist/vue-about-me.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "browser": {
    "./sfc": "./src/VueAboutMe.vue"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:example": "vite build --mode example",
    "lint": "eslint .",
    "release": "bumpp",
    "serve": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "sass": "^1.83.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@iconify-json/ri": "^1.2.5",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/compiler-sfc": "^3.5.13",
    "@vueuse/core": "^12.2.0",
    "bumpp": "^9.9.2",
    "eslint": "^9.17.0",
    "eslint-plugin-format": "^0.1.3",
    "typescript": "^5.7.2",
    "unplugin-icons": "^0.22.0",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.0.6",
    "vite-plugin-dts": "^4.4.0",
    "vue-tsc": "^2.2.0"
  }
}
