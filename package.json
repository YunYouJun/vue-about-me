{
  "name": "vue-about-me",
  "version": "0.0.5",
  "description": "Use Vue Element Display About Me Info",
  "main": "dist/vue-about-me.min.js",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/YunYouJun/vue-about-me.git",
  "author": "YunYouJun <me@yunyoujun.cn>",
  "license": "MIT",
  "scripts": {
    "dev": "npm run example",
    "build": "bili src/index.js --plugin vue --format umd,umd-min,cjs,es --exports named --module-name VueAboutMe",
    "prepublish": "npm run build",
    "example": "poi example/index.js",
    "build:example": "poi build example/index.js --out-dir example/dist --public-path ./ --source-map false",
    "predeploy": "npm run build:example",
    "deploy": "gh-pages -d example/dist"
  },
  "keywords": [
    "vue",
    "about",
    "me",
    "link",
    "copyright"
  ],
  "devDependencies": {
    "babel-plugin-component": "^1.1.1",
    "bili": "^3.1.1",
    "element-theme-ink": "^1.0.0",
    "node-sass": "^4.9.0",
    "poi": "^10.1.9",
    "rollup-plugin-vue": "^4.1.4",
    "sass-loader": "^7.0.1"
  },
  "dependencies": {
    "v-tooltip": "^2.0.0-rc.33"
  }
}
