{
  "name": "vue-about-me",
  "version": "1.2.4",
  "description": "A Vue component for displaying About Me Info.",
  "main": "./dist/vue-about-me.cjs",
  "module": "./dist/vue-about-me.mjs",
  "types": "index.d.ts",
  "files": [
    "dist",
    "src",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-about-me.mjs",
      "require": "./dist/vue-about-me.cjs"
    },
    "./style.css": "./dist/style.css",
    "./sfc": {
      "import": "./src/VueAboutMe.vue"
    }
  },
  "browser": {
    "./sfc": "./src/VueAboutMe.vue"
  },
  "style": "./dist/style.css",
  "repository": "https://github.com/YunYouJun/vue-about-me.git",
  "author": {
    "email": "me@yunyoujun.cn",
    "name": "YunYouJun",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:example": "vite build --mode example",
    "serve": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "about",
    "me",
    "link",
    "copyright"
  ],
  "devDependencies": {
    "@iconify-json/ri": "^1.1.1",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/compiler-sfc": "^3.2.31",
    "@vueuse/core": "^8.0.0",
    "typescript": "^4.6.2",
    "unplugin-icons": "^0.13.3",
    "unplugin-vue-components": "^0.18.0",
    "vite": "2.8.6"
  },
  "dependencies": {
    "sass": "^1.49.9",
    "vue": "^3.2.31"
  }
