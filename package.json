{
  "name": "vue-about-me",
  "type": "module",
  "version": "1.3.3",
  "packageManager": "pnpm@9.12.0",
  "description": "A Vue component for displaying About Me Info.",
  "author": {
    "email": "me@yunyoujun.cn",
    "name": "YunYouJun",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MIT",
  "repository": "https://github.com/YunYouJun/vue-about-me.git",
  "keywords": [
    "vue",
    "about",
    "me",
    "link",
    "copyright"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-about-me.mjs",
      "require": "./dist/vue-about-me.cjs"
    },
    "./style.css": "./dist/style.css",
    "./sfc": "./src/VueAboutMe.vue"
  },
  "main": "./dist/vue-about-me.cjs",
  "module": "./dist/vue-about-me.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "browser": {
    "./sfc": "./src/VueAboutMe.vue"
  },
  "style": "./dist/style.css",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:example": "vite build --mode example",
    "lint": "eslint .",
    "release": "bumpp",
    "serve": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "sass": "^1.79.4",
    "vue": "^3.5.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@iconify-json/ri": "^1.2.0",
    "@types/node": "^22.7.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/compiler-sfc": "^3.5.11",
    "@vueuse/core": "^11.1.0",
    "bumpp": "^9.6.1",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "typescript": "^5.6.2",
    "unplugin-icons": "^0.19.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.3",
    "vue-tsc": "^2.1.6"
  }
}
